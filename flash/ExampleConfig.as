package {	import flash.display.MovieClip;	import fl.controls.TextInput;	import flash.events.Event;		import SharedPreferences;		public class ExampleConfig extends MovieClip	{		public var			message_TextInput:TextInput;					public function ExampleConfig()		{			message_TextInput = this["mc_message_TextInput"];			message_TextInput.text = SharedPreferences.getInstance().displayString;            message_TextInput.addEventListener(Event.CHANGE, textEntered);		}				public function textEntered(e:Event):void		{			SharedPreferences.getInstance().displayString = message_TextInput.text;			SharedPreferences.getInstance().setNeedsUpdate();		}	}}